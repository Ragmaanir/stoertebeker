{
  "name": "stoertebeker",
  "version": "0.1.0",
  "flat": "true",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node_modules/.bin/electron .built/index.js",
    "test": "rm -rf .built_tests && cd test && tsc && ava --verbose --timeout=2s"
  },
  "ava": {
    "files": [
      ".built_tests/test/**/*.test.js"
    ],
    "source": [
      ".built_tests/src/**/*.js"
    ],
    "concurrency": 1,
    "failFast": true
  },
  "devDependencies": {
    "@types/node-ipc": "^9.1.0",
    "@types/power-assert": "^1.4.29",
    "ava": "^0.25.0",
    "awesome-typescript-loader": "^3.4.1",
    "power-assert": "^1.4.4",
    "source-map-loader": "^0.2.3",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "electron": "^3.0.5",
    "node-ipc": "^9.1.1"
  }
}
